}

/*menu*/
.menu>div{background: url(../app/img/left_menu.png);background-size: 259px 44px;height: 44px;line-height: 30px;font-size: 16px;}
.menu>div .field-div{margin:6px 10px 6px 10px;}
.field-div{-webkit-box-shadow: rgb(102, 102, 102) 1px 1px 4px 0px inset;height: 30px;background: #fff;border-radius: 255px;line-height: 32px;color: #ccc;}
.menu>div>span{left: 60%;position: absolute;}
.menu .icon {font-size: 1.2em;line-height: 30px;vertical-align: middle;color: #e0e0e0;}
.menu a{font-size:16px;display:block;-webkit-user-select: none;color: #fff;}
.menu li{line-height: 44px;text-align: left;font-size:16px;padding-left: 2em;background:url(../app/img/left_menu.png);background-size:259px 44px;}
.menu li span {margin-right: .8em;}
.menu li.clicked,.menu li:active {background: url(../app/img/left_menu_click.png);}
.menu li.anchor {height: 9px;background: url(../app/img/left_bg.png);background-size: 100% 9px;}

/*index*/
header {
	-webkit-box-shadow: 0px 0px 6px 1px #aaa;
	-moz-box-shadow: 0px 0px 6px 1px #aaa;
	-ms-box-shadow: 0px 0px 6px 1px #aaa;
	box-shadow: 0px 0px 6px 1px #aaa;
	height: 44px;
	background: #fff;
	border-top-right-radius: 5px;
	border-top-left-radius: 5px;
}
.icon{font-size: 1.7em;}
header a{display: block;width: 50px;text-align: center; z-index: 2;}

header .title{background: url(../app/img/title_bg_640.png) no-repeat center 0px;line-height: 40px;font-size: 1.2em; color:#fff;background-size:120px 44px;text-shadow: 0px -1px 0px #731C14;}
.index ul{width:500%;height:100%}
.index ul>li{width:20%;height:100%;float:left;overflow: hidden;background: #fff;white-space: normal;}
.index ul>li h2{margin: 0;font-weight: bold;font-size: 1.1em;line-height: 1.3em;letter-spacing: 0.05px;color: #2B2B2B;}
.index ul>li .ios h2{font-weight: bold;font-size: 1.2em;padding:5px 0;}
.index ul>li .readed h2{color:#999999}
.indexscroller {width: 500%;height: 100%;float: left;padding: 0;}
article.index{position:absolute; bottom:0; top:0;}
.scrollpage{width:100%;position:absolute; bottom:0; top:44px;}
p.info{padding: 0;margin: auto;color: #ccc;}
p.info span{padding:0px 3px 0px 1px; font-size:13px;}
.container>div {border-bottom: 2px solid #f4f4f4;margin: 0px 10px 0px 10px;font-size: 1.1em; overflow: hidden}
.container>div.ios{margin: 0px 13px 0px 13px;}
.pullDown,.pullUp {text-align: center;height: 48px;line-height: 48px;display:none;}
.pullDown .pullDownIcon{width: 38%;height: 48px;display: block;float: left;line-height: 48px;}
.pullDownIcon img{float: right; }
img.touch {-webkit-transition: -webkit-transform 0.3s ease-in; -webkit-transform: rotate(0);}
img.pull{-webkit-transition: -webkit-transform 0.3s ease-in; -webkit-transform: rotate(180deg)}
img.refresh{-webkit-animation: spin 1s infinite linear;margin-top: 8px;}
@-webkit-keyframes spin{
    0% {
        -webkit-transform:rotate(0deg);
    }
    100% {
        -webkit-transform:rotate(360deg);
    }
}
.pullDown .pullDownLabel{float: right;width: 60%;text-align: left;}
.container>img{width:100%; height:100%; border:0px;}
#container>li>section{height:80px;}
.container {background:#fcfcfc;}
.index-loadding{margin:auto;margin-top: 30%;border-bottom:none;}
.container:not(.line)>div:active{background:#f4f4f4;}
.loadingpage{position: absolute;width: 100%;height:100%;opacity:1;margin-top: 10%;}
.loadingpage>div{background: url(../app/img/load.png) top no-repeat;width: 100%;height: 100%;margin-top: 20%; background-size:70px 42px}
.index.loadingpage{background: url(../app/img/loading.png) no-repeat center;width: 100%;height: 100%}

div.ybg{width: 100%;height: 8px;background: url(../app/img/ybg.png) repeat;position: absolute;top: 44px;z-index:1000}


.index-loadding>div:first-child{width: 38%;float:left; height:24px;background: url(../app/img/loading-gif.gif) no-repeat center right;background-size:16px 16px;}
