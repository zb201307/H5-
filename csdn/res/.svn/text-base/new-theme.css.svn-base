/* header */
#write ,#login
{
    width:100%;
    height:100%;
    background:#fff;
    position:relative;
    top:100%; 
    -webkit-transition-duration: 1s;
    z-index:1001;
    display:none;
}
#write .header ,#login .header
{
    width:100%;
    height:40px;
    background:#fff;
    -webkit-box-shadow:0px 1px 6px #aaa;
	-moz-box-shadow:0px 1px 6px #aaa;
	box-shadow:0px 1px 6px #aaa;
}
#write .header .left, #login .header .left
{
    float:left;
    width:50px;
    height:40px;
    line-height:40px;
    vertical-align:middle;
    text-align:center;
}
#write .header .left:active, #login .header .left:active,#write .header .right:active, #login .header .right:active
{
    background:#eee;
}
#write .header .center,#login .header .center
{
    background: url(../app/img/title_bg_640.png) no-repeat center 0px;
    line-height: 40px;
    font-size: 1.2em;
    color:#fff;
    background-size:120px 44px;
    text-shadow: 0px -1px 0px #731C14;
    width: 100%;
    text-align: center;
}




#write .header .right,#login .header .right
{
    float:right;
    width:50px;
    height:40px;
    line-height:40px;
    vertical-align:middle;
    text-align:center;
}

#write .show, #login form
{
    padding:20px;
}
#write textarea
{
    width:100%;
    height:80px;
    -webkit-box-shadow:inset 0 2px 6px rgba(0,0,0,0.1);
	-moz-box-shadow:inset 0 2px 6px rgba(0,0,0,0.1);
	box-shadow:inset 0 2px 6px rgba(0,0,0,0.1);
	border-color:#ababab;
	font-size:1.2em;
}
/*��¼*/
#login input
{
    -webkit-box-shadow:inset 0px 1px 1px rgba(211,211,211,.5);
    -moz-box-shadow:inset 0px 1px 1px rgba(211,211,211,.5);
    box-shadow:inset 0px 1px 1px rgba(211,211,211,.5);
    border:1px solid #e0e0e0;
    -webkit-border-radius:4px;
	-moz-border-radius:4px;
	border-radius:4px;
    height:30px;
    line-height:30px;/*IOS5.1.1 ����placeholder���²��������⣬��Ҫhack*/
    width:100%;
    margin-bottom:10px;
}


/*placeholder color*/
#login input::-webkit-input-placeholder {
    color:#e0e0e0;
    font-size:1.3em;
    padding-left:8px;
}
#login input:-moz-placeholder {
    color:#e0e0e0;
    font-size:1.3em;
    padding-left:8px;
}
