pre,span,strong{white-space: pre-wrap; white-space:-moz-pre-wrap; white-space:-pre-wrap; white-space:-o-pre-wrap; word-wrap:break-word;word-break: break-word;}
ol,li,p{word-break: break-word;}
html {
	position:relative;
	margin:0;
	padding:0;
	height:100%;
}
body {
	font-size:13px;
	margin:auto;
	height:100%;
	overflow: hidden;
	/*color:#666666;*/
	-webkit-user-select: none;
	position:relative;
	display:box;
	font-family: Arial;
	-webkit-touch-callout:none;
	-webkit-box-orient:vertical;
	-webkit-box-align:stretch;
	-webkit-tap-highlight-color:rgba(0,0,0,0);
	-ms-touch-action: none;
}
#container>li, body{background-color:#f0f0f0}

article{width: 100%;}
a{text-decoration: none; color:#ccc}
ul#container,ul#menu-ul{list-style-type:none;margin:0px;padding: 0;}
.con.news_content{line-height: 150%}

.opactiychange{
	transition: opacity 0.1s ease-in;
	-webkit-transition: opacity 0.1s ease-in;
}
.bottomchange{
	transition: bottom 0.5s ease-in;
	-webkit-transition: bottom 0.5s ease-in;
}
.heightchange{
	transition: height 0.5s ease-in;
	-webkit-transition: height 0.5s ease-in;
}

